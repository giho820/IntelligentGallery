package kr.ac.korea.intelligentgallery.common.view.textview;

import android.content.Context;
import android.util.AttributeSet;
import android.widget.TextView;

import kr.ac.korea.intelligentgallery.util.TextUtil;

public class TextViewNotoSansCJKkrMedium extends TextView {

    public TextViewNotoSansCJKkrMedium(Context context, AttributeSet attrs) {
        super(context, attrs);
        if (TextUtil.NotoSansCJKkrMedium != null)
            setTypeface(TextUtil.NotoSansCJKkrMedium);
    }

    public TextViewNotoSansCJKkrMedium(Context context) {
        super(context);
        if (TextUtil.NotoSansCJKkrMedium != null)
            setTypeface(TextUtil.NotoSansCJKkrMedium);
    }

    public TextViewNotoSansCJKkrMedium(Context context, AttributeSet attrs, int defStyle) {
        super(context, attrs, defStyle);
        if (TextUtil.NotoSansCJKkrMedium != null)
            setTypeface(TextUtil.NotoSansCJKkrMedium);
    }

}
